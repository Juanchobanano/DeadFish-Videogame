            <string>if(game_start){
    /* Sombrilla */
    if(!muerepajaro){
        if(tipo==4){
            vspeed = -8.2;
        }else if(tipo==3){
            vspeed = -7.8
        }
    }else{
        if(tipo==4){
            gravity = 1;
        }else if(tipo==3){
            gravity = .5;
        }
    }
    
    if(self.y&lt;=-63){
        Score++;
        // Logro por Score.
        if(Score==10){
            achievement_post("CgkIqsity4oFEAIQAg",100);
        }else if(Score==20){
            achievement_post("CgkIqsity4oFEAIQAw",100);
        }else if(Score==30){
            achievement_post("CgkIqsity4oFEAIQBA",100);
        }else if(Score==40){
            achievement_post("CgkIqsity4oFEAIQBQ",100);
        }         
        if(!pausarmusica){
             audio_play_sound(sn_boton,2,0);
        }
       // obj_score.image_index = Score;
        self.y = 770;
        activar_animacion = 1;
    }
    if(self.y&gt;=800){
        with obj_control_enemys instance_destroy();
        game_start = 0;
        muerepajaro = 1;
        if(!pausarmusica){
             if(contadormuerte==0){
                if(sound_exists(sn_golpe)){
                    audio_stop_sound(sn_golpe);
                }
                audio_play_sound(sn_piso,2,0);
                contadormuerte++;
             }
        }
        if(!instance_exists(obj_gameover)){
          instance_create(81,190,obj_gameover);
        }
        if(contador==0){
            alarm[0] = 60;
            contador++;
        }
    }
    if(color&gt;1){
        background_blend[0] = make_colour_rgb(255,color,255);
        color -= .03;
    }
    if(activar_animacion){
        if(instance_exists(obj_flecha)){
            obj_flecha.image_alpha -= .1;
            if(obj_flecha.image_alpha&lt;=0){
                with obj_flecha instance_destroy();
            }
        }
    }
    if(animar){
        image_angle -= 10;
    }
    if(self.x &lt; 0 || self.x &gt; 480){
        room_goto(Room_End);   
    }
}
