    if(image_xscale == -1){
        image_angle = 90;
    }else{
        image_angle = 270;
    }
    puntos = false;
    
    // Pez sorpresa!
    if(image_index == 6){
        // Elegir una bonificación.
        switch(choose("tiros","vidas","tiempo")){
            case "tiros":
                municion += 20;
            break;
            case "vidas":
                vidas += 2;
            break;
            case "tiempo":
                tiempo += 2;
            break;
        }
        instance_destroy();
    }else{
        set_movement_direction_speed(angulo,velocidad);    
    }
    
    // Disparo.
    if(!mejora_arma){
        audio_play_sound(sound_weapon,1,0);
        mouse_clear(mb_left);
    }else{
        if(municion &gt; 0){
            disparos++;
            municion--;
        }else{
            mejora_arma = 0;
        }
        
        if(disparos &gt;= 4){
            disparos = 0;
            mouse_clear(mb_left);
        }
                       
        audio_play_sound(sound_weapon,1,0);
    }
}
